(*# MixedStateChoice #*)

global protocol Mixed (role A, role B, role C) {
  choice at A {
    M1 from A to B ; M1F from B to C ;
  } or {
    M2 from A to B ; M2F from B to C ;
  } or {
    M3 from A to C ; M3F from C to B ; (*) here B learns of the choice indirectly from C (there is no A to B message)
  }
}
